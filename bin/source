from typing_extensions import Type

# Import the library
#from desoper import abc123# debo importar funciones del archivo pc 
import argparse
import sys 
def iniciar():

    parser = argparse.ArgumentParser(description='argumentos de pedida del programa')

    # entrada argumento numero 1 
    parser.add_argument('-N', '--numlist',type= int,default=500000,required=True,help='espacio de listas de posibles repuestas')
    # entrada argumento numero 2 
    parser.add_argument('-m', '--magnitud',type=int,default=1,required=True,help='agregue enteros mayores que 1 hasta que obtenga soluciones completas ')
    # entrada argumento numero 3 
    parser.add_argument('-zmax', '--zmaximo',type=int,default=30,required=False,help='valor del zmaximo')
    # entrada argumento numero 4 
    parser.add_argument('-imax', '--imaximo',type=int,default=0, required=False, help='valor de iteraciones hechas para recoorre el espacio de posib√±es respuestas')
    # entrada argumento numero 5
    parser.add_argument('-n', '--number',type=int,default=5,required=True,action='store',help='entero para que es la dimension de z ')

    args = parser.parse_args()
    return args

def main(args): 
  
  if args.number:
      a=args.number*3
  return  print(a) 
 
if __name__ == '__main__': 
    args=iniciar()
    main(args)

